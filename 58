class Solution {
  int lengthOfLastWord(String s) {
    s = s.trim(); 
    List<String> words = s.split(' ');

    return words.last.length;
  }

  void main() {
    print(lengthOfLastWord('Hello World'));
    print(lengthOfLastWord('Fly me to the moon'));
    print(lengthOfLastWord('luffy is still joyboy'));
  }
}